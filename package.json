{
  "name": "roof-restore-pro",
  "version": "1.0.0",
  "description": "A modern web application for connecting homeowners with roofing professionals",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "react-scripts start",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build": "react-scripts build",
    "heroku-postbuild": "npm run install-client && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "roof",
    "maintenance",
    "home repair",
    "inspection"
  ],
  "author": "QRUMN",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^6.1.10",
    "@mui/material": "^5.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "aws-sdk": "^2.1001.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "pdfkit": "^0.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "socket.io": "^4.3.1",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
